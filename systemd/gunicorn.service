[Unit]
Description=Gunicorn daemon for Django
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/var/www/corp-admin/backend/django_app
ExecStart=/var/www/corp-admin/backend/django_app/venv/bin/gunicorn --workers 3 --bind 127.0.0.1:8000 django_app.wsgi:application

[Install]
WantedBy=multi-user.target
